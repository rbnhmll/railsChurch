<p id="notice"><%= notice %></p>
<%= image_tag @post.lead_image.url :full %>
<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= raw @post.content %>
</p>

<% if user_signed_in? %>
	<p>
	  <strong>Active:</strong>
	  <%= @post.active %>
	</p>

	<%= link_to 'Edit', edit_post_path(@post) %> |
<% end %>

<%= link_to 'Back', posts_path %>